<p id="notice"><%= notice %></p>

<h1>Voters</h1>

<table>
  <thead>
    <tr>
      <th>Voterid</th>
      <th>Last name</th>
      <th>First name</th>
      <th>Middle name</th>
      <th>Address</th>
      <th>City</th>
      <th>State</th>
      <th>Zip</th>
      <th>Email</th>
      <th>Phone</th>
      <th>Date of birth</th>
      <th>Party affiliation</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @voters.each do |voter| %>
      <tr>
        <td><%= voter.voterID %></td>
        <td><%= voter.last_name %></td>
        <td><%= voter.first_name %></td>
        <td><%= voter.middle_name %></td>
        <td><%= voter.address %></td>
        <td><%= voter.city %></td>
        <td><%= voter.state %></td>
        <td><%= voter.zip %></td>
        <td><%= voter.email %></td>
        <td><%= voter.phone %></td>
        <td><%= voter.date_of_birth %></td>
        <td><%= voter.party_affiliation %></td>
        <td><%= link_to 'Show', voter %></td>
        <td><%= link_to 'Edit', edit_voter_path(voter) %></td>
        <td><%= link_to 'Destroy', voter, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Voter', new_voter_path %>
